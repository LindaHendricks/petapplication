<h1>Here is my booking Form page</h1>


<% if @errors %>

<% @errors.each do |error| %>
<p><%= error%></p>
<% end %>
<% end %>

<%=form_for @booking do |f| %>

<%=f.label :date %>
<%=f.text_field :date %>

<%=f.label :location %>
<%=f.text_field :location%>

<%=f.label :renter_id %>
<%=f.collection_select :renter_id, @renters, :id, :username%>

<%=f.label :pet_id %>
<%=f.collection_select :pet_id, @pets, :id, :name%>

<%=f.submit "Submit"%>

<%end%>